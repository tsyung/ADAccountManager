<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Account.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.7.1.min.js")" type="text/javascript"></script>
    @*<script src="@Url.Content("~/Scripts/modernizr-2.5.3.js")" type="text/javascript"></script>*@
    <!--[if lt IE 9]> 
    <script src="@Url.Content("~/Scripts/html5shiv.js")" type="text/javascript"></script>
    <![endif]-->
    @RenderSection("HeaderSection", required: false) 
</head>
<body>
    <header>
        <h1>AD域服务帐号管理</h1><h2>@ViewBag.Title</h2>
    </header>
    <section>
        @if (!string.IsNullOrWhiteSpace(ViewBag.Message))
        {
            <div class="message">@ViewBag.Message</div>
        }
        @if (!string.IsNullOrWhiteSpace(ViewBag.ErrorMessage))
        {
            <pre class="error">@ViewBag.ErrorMessage</pre>
        }
        @RenderBody()
    </section>
    <footer>Thanks, enjoy coding!</footer>
</body>
</html>
